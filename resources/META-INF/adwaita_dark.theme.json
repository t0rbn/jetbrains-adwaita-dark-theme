{
  "name": "adwaita-dark",
  "dark": true,
  "author": "t0rbn",
  "editorScheme": "/META-INF/adwaita_dark.xml",
  "colors": {
    "backgroundDefault": "#323232",
    "backgroundDarker": "#2D2D2D",
    "foregroundDefault": "#c0c1c4",
    "foregroundLighter": "#ffffff",
    "foregroundDarker": "#7f7f7f",
    "accent": "#15539E",
    "accentSecondary": "#E9AD0C"
  },
  "ui": {
    "*": {
      "background": "backgroundDefault",
      "foreground": "foregroundDefault",
      "infoForeground": "accentSecondary",

      "selectionBackground": "accent",
      "selectionForeground": "foregroundLighter",
      "selectionInactiveBackground": "accent",
      "selectionInactiveForeground": "foregroundLighter",

      "disabledBackground": "backgroundDarker",
      "inactiveBackground": "backgroundDefault",
      "inactiveForeground": "foregroundDefault",

      "disabledForeground": "foregroundDarker",
      "disabledText": "foregroundDarker",

      "acceleratorForeground": "#E6E6E6",
      "acceleratorSelectionForeground": "#E6E6E6",

      "errorForeground": "#C01C28",

      "borderColor": "backgroundDefault",
      "disabledBorderColor": "#AA6E28",
      "focusColor": "accent",
      "focusedBorderColor": "accent",
      "hoverBorderColor": "accentSecondary",
      "pressedBorderColor": "accentSecondary",

      "separatorColor": "backgroundDarker",
      "lineSeparatorColor": "#backgroundDarker"
    }
  }
}
